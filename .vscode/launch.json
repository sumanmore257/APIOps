{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/api_management"
        },
        {
            "name": "Python: Debug Pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "tests/integeration"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/api_management"
        }
    ]
}
